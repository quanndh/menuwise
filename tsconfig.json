{
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["es6", "dom", "es2017"],
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "allowJs": true,
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true
  }
}
